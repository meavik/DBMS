DECLARE
GRADE CHAR(1) := 'U';
BEGIN
CASE GRADE
WHEN 'A' THEN
DBMS_OUTPUT.PUT_LINE('EXCELLENT');
WHEN 'B' THEN
DBMS_OUTPUT.PUT_LINE('VERY GOOD');
WHEN 'C' THEN
DBMS_OUTPUT.PUT_LINE('GOOD');
WHEN 'D' THEN
DBMS_OUTPUT.PUT_LINE('AVERAGE');
WHEN 'F' THEN
DBMS_OUTPUT.PUT_LINE('PASSED WITH GRACE');
ELSE
DBMS_OUTPUT.PUT_LINE('FAILED');
END CASE;
END;
